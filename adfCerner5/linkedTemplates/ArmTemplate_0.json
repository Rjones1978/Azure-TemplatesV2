						"folderPath": {
							"value": "@dataset().STR_Directory",
							"type": "Expression"
						},
						"fileSystem": {
							"value": "@dataset().STR_Container",
							"type": "Expression"
						}
					},
					"compressionCodec": {
						"value": "@dataset().STR_Compression",
						"type": "Expression"
					}
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LS_ADL2')]"
			]
		},
		{
