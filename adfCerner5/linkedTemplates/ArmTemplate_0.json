						},
						"secretName": {
							"value": "[parameters('LS_ORACLE_properties_typeProperties_connectionString_secretName')]",
							"type": "Expression"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LS_KV_ConnectionStrings')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LS_SQL')]",
