					"STR_BlobSecret": {
						"type": "string"
					},
					"STR_BlobContainer": {
						"type": "string"
					},
					"STR_BlobDirectory": {
						"type": "string"
					},
					"STR_BlobFileName": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "Json",
