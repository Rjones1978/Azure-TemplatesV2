		{
			"name": "[concat(parameters('factoryName'), '/LS_ADL2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"STR_SecretName": {
						"type": "string",
						"defaultValue": "SEC-CernerDataLake2"
