					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": {
							"value": "@dataset().STR_BlobFileName",
							"type": "Expression"
						},
						"folderPath": {
							"value": "@dataset().STR_BlobDirectory",
							"type": "Expression"
						},
						"container": {
							"value": "@dataset().STR_BlobContainer",
							"type": "Expression"
						}
					}
				},
				"schema": {}
			},
			"dependsOn": [
